#include <bits/stdc++.h>
using namespace std;

int main() {
	
	cout << "Enter a number to print the hollow Rectangle: ";
	int n;
	cin >> n;
	
	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= n; j++) {
			if (i == 1 || i == n || j == 1 || j== n) cout << "*";
		}
		cout << endl;
	} 
}

