// Binary Search
// Elements must be sorted
#include <bits/stdc++.h>
using namespace std;

int search(vector<int> &arr, int target) {
	int start = 0, end = arr.size() - 1;
	while (start <= end) {
		int mid = (start + end) / 2;
		if (arr[mid] == target) return mid;
		else if (arr[mid] > target) end = mid - 1;
		else start = mid + 1;
	}
	return - 1;
}

int main() {
	vector<int> arr = {5, 1, 4, 2};
	int res = search(arr, 4);
	cout << res;
}