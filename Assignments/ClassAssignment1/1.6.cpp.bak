// Diagonal Sum of a Matrix
#include <bits/stdc++.h>
using namespace std;

int diagonalSum(vector<vector<int>>& arr) {
	int sum = 0;
	for (int i = 0; i < arr.size(); i++) {
		for (int j = 0; j < arr[0].size(); j++) {
			if (i == j) sum += arr[i][j];
		}
	}
	return sum;
}

int main() {
	vector<vector<int>> arr = [[1, 2, 3], [4, 5, 6], [7, 8, 9]];
	int res = diagonalSum(arr);
	cout << res;
}