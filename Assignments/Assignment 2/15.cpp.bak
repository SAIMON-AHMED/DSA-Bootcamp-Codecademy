// Array Leaders
#include <bits/stdc++.h>
using namespace std;

vector<int> leaders(vector<int>& arr) {
  vector<int> ans;
  int len = arr.size();
  int subtract = arr[len - 1];
  for (int i = len - 2; i > 0; i--) {
    if (arr[i] > subtract) {
      ans.push_back(arr[i]);
      subtract += arr[i];
		}
  }
  ans.push_back(arr[len - 1]);
  return ans;
}

int main() {
	vector<int> arr = {16, 17, 4, 3, 5, 2};
	vector<int> ans = leaders(arr);
	for (int n: ans) cout << n << " ";
}