#include <bits/stdc++.h>
using namespace std;
    int binarySearch(int x, int start, int end) {
        int mid;
        while (start <= end) {
            mid = start + (end - start) / 2;
            if (mid * mid <= x && (mid + 1) * (mid + 1) > x) {
                return mid;
            } else if (mid * mid < x) {
                start = mid + 1;
            } else {
                end = mid - 1;
            }
        }
        return mid;
    }
    int main() {
			int x = 9;
        if (x == 0 || x == 1) return x;
        int start = 1, end = x/2;
        int res = binarySearch(x, start, end);
				cout << res;
    }
